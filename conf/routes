# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                    controllers.Application.index()

GET         /projects            controllers.Application.getProjects

GET         /projects/new        controllers.Project.foo()
POST        /projects/data       controllers.Project.addProjectData()
POST        /projects/contact    controllers.Project.addContact()
POST        /projects/ware       controllers.Project.addWaren()
POST        /projects/save       controllers.Project.saveProject()

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.at(path="/public", file)
